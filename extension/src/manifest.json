{
  "manifest_version": 3,
  "name": "SafeChat AI Scanner",
  "version": "1.0",
  "description": "scan chrom for dangerous content with  AI help",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scanner.js"]
    }
  ]
}